<% } else if (section === 'view_task') { %>
    <div class="main-content">
        <div class="dashboard-breadcrumb mb-25">
            <h2>Task Management</h2>
          
        </div>
        <div class="row">
            <div class="col-12">
                <div class="panel">
                    
                    <div class="panel-body">
                        <form action="/updateTask/<%= data.id %>" method="POST">
                            <div class="row g-3">
                                <div class="col-xxl-12 col-lg-12 col-sm-12">
                                    <label class="form-label">Description</label>
                                    <textarea name="description" id="taskTextarea" class="form-control form-control-sm" rows="15" cols="50" style="width: 100%;" onchange="updateDescription(this.value)"><%= data.description %></textarea>
                                </div>
                            </div>
                        
                            <script>
                                const updateDescription = async (newDescription) => {
                                    try {
                                        const id = '<%= data.id %>'; // Assuming you're passing the task ID to the view
                                        const response = await fetch(`/updateTask/${id}`, {
                                            method: 'PUT',
                                            headers: {
                                                'Content-Type': 'application/json',
                                            },
                                            body: JSON.stringify({ description: newDescription }),
                                        });
                            
                                        if (!response.ok) {
                                            throw new Error('Failed to update task description');
                                        }
                            
                                        // No need to log data
                                    } catch (error) {
                                        console.error('Error updating task description:', error);
                                        // Handle error if needed
                                    }
                                }
                            </script>
                            
          
                        </form>
                        
                    </div>
                </div>
            </div>

            
