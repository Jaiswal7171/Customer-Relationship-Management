                            <div class="row g-3">
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Company Name</label>
                                    <input type="text" name="companyName" class="form-control form-control-sm" required>
                                </div>

                                <div class="col-xxl-4 col-lg-4 col-sm-4" hidden>
                                    <label class="form-label">D_ID</label>
                                    <input type="number" name="d_id" value="1" class="form-control form-control-sm" required >
                                </div>


                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Company Mobile</label>
                                    <input type="text" name="companyMobile" class="form-control form-control-sm" pattern="[6-9]\d{9}" title="Please enter a 10-digit number starting with 6, 7, 8, or 9." required>
                                    <sub><i class="text-danger">Only allowed 10 Digit No. Starting With 6 to 9</i></sub>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">WhatsApp Number</label>
                                    <input type="text" name="whatsappNumber" class="form-control form-control-sm" pattern="[6-9]\d{9}" title="Please enter a 10-digit number starting with 6, 7, 8, or 9." required>
                                    <sub><i class="text-danger">Only allowed 10 Digit No. Starting With 6 to 9</i></sub>
                                </div>

                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Company Email</label>
                                    <input type="email" name="email" class="form-control form-control-sm" title="Please enter a valid email address" required>
                                </div>
                                


                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Company Category</label>
                                    <input type="text" name="companyCategory" class="form-control form-control-sm" pattern="[A-Za-z\s]+" title="Only letters and spaces are allowed" required>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Address</label>
                                    <input type="text" name="address" class="form-control form-control-sm" required>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
