                                    <input type="text" name="city" pattern="[A-Za-z\s]+" title="Only letters and spaces are allowed" class="form-control form-control-sm" required>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">State</label>
                                    <input type="text" name="state" pattern="[A-Za-z\s]+" title="Only letters and spaces are allowed" class="form-control form-control-sm" required>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Employee ID</label>
                                    <input type="text" name="employeeId" value="<%= employeeid.id %>" class="form-control form-control-sm" required readonly>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4">
                                    <label class="form-label">Call Date</label>
                                    <input type="date" name="callDate" class="form-control form-control-sm" required>
                                </div>
                                <div class="col-xxl-4 col-lg-4 col-sm-4" style="display: none;">
                                    <label class="form-label">Call Time</label>
                                    <input type="text" name="callTime" class="form-control form-control-sm" value="">
                                </div>
                            </div>
                            <div class="row mt-3">
                                <div class="col-12 d-flex justify-content-end">
                                    <button type="submit" class="btn btn-sm btn-success">Save Customer</button>
                                </div>
                            </div>
                        </form>
                        
                        
                    </div>
                </div>
            </div>
     
